{
  "name": "uber-eats-clone",
  "version": "1.0.0",
  "description": "A full-stack food delivery application with multiple user roles",
  "main": "backend/index.js",
  "scripts": {
    "start": "node backend/index.js",
    "server": "nodemon backend/index.js",
    "client": "cd frontend && npm run dev",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd frontend && npm run build",
    "test": "jest"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.5.0",
    "jsonwebtoken": "^9.0.1",
    "pm2": "^5.4.3",
    "punycode": "^2.3.1",
    "socket.io": "^4.7.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.4"
  }
}
